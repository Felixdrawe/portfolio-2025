{
  "name": "portfolio-2025",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "production": "next build && next start",
    "check-case": "find . -type f -name \"*.tsx\" -o -name \"*.ts\" -o -name \"*.js\" -o -name \"*.jsx\" | sort -f | uniq -i -d",
    "list-components": "find ./components -type f | sort",
    "list-app": "find ./app -type f | sort",
    "check-imports": "grep -r \"import.*from.*components\" ./app ./components --include=\"*.tsx\" --include=\"*.ts\"",
    "check-skills": "grep -r \"import.*from\" ./components/Skills.tsx",
    "view-skills": "cat ./components/Skills.tsx"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.15.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
